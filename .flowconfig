[ignore]
.*/node_modules/@parcel/.*
.*/node_modules/parcel-bundler/.*

[include]

[libs]

[lints]
deprecated-utility=off
implicit-inexact-object=error

[options]
server.max_workers=1
exact_by_default=true

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
