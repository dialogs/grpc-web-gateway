{
  "name": "@dlghq/web-platform-grpc-web-gateway",
  "private": true,
  "version": "3.0.11",
  "description": "Dialog Web Platform: gRPC Web Gateway",
  "author": "Nikita Gusakov <nkt@dlg.im>",
  "contributors": [
    "Mikhail Nasyrov <mikhail@mnasyrov.com>"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@dlghq/grpc-web-gateway": "3.0.11",
    "cors": "2.8.5",
    "env-schema": "2.0.1",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@dlghq/grpc-web-gateway-signaling": "3.0.11",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "nodemon": "2.0.5",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "scripts": {
    "dev": "nodemon --exec 'ts-node' src/index.ts",
    "start": "node dist/src/index.js",
    "clean": "rimraf dist",
    "build": "tsc --build"
  },
  "nodemonConfig": {
    "ignoreRoot": [
      ".git"
    ],
    "watch": [
      "src/**/*.ts",
      "node_modules/@dlghq/grpc-web-gateway"
    ]
  }
}
