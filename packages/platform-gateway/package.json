{
  "name": "@dlghq/web-platform-grpc-web-gateway",
  "private": true,
  "version": "4.0.0-alpha.10",
  "description": "Dialog Web Platform: gRPC Web Gateway",
  "author": "Nikita Gusakov <nkt@dlg.im>",
  "contributors": [
    "Mikhail Nasyrov <mikhail@mnasyrov.com>"
  ],
  "engines": {
    "node": ">=12"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "isLibrary": false,
      "includeNodeModules": true
    }
  },
  "scripts": {
    "dev": "nodemon --exec 'ts-node' src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "parcel build . --no-cache --no-scope-hoist"
  },
  "nodemonConfig": {
    "ignoreRoot": [
      ".git"
    ],
    "watch": [
      "src/**/*.ts",
      "node_modules/@dlghq/grpc-web-gateway"
    ]
  },
  "dependencies": {
    "@dlghq/grpc-web-gateway": "4.0.0-alpha.10",
    "cors": "2.8.5",
    "engine.io": "4.0.0",
    "env-schema": "2.0.1",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@dlghq/grpc-web-gateway-signaling": "4.0.0-alpha.10",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "nodemon": "2.0.5",
    "parcel": "2.0.0-nightly.427",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  }
}
