{
  "name": "@dlghq/grpc-web-gateway",
  "version": "1.0.0-beta.14",
  "description": "gRPC to json gateway",
  "main": "dist/index.js",
  "scripts": {
    "test": "exit 0",
    "generate-example": "pbjs -t static-module --es6 -o example/client/api.gen.js example/proto/*.proto",
    "generate-signaling-js": "pbjs -t static-module --es6 -o src/shared/signaling.js src/shared/signaling.proto",
    "generate-signaling-ts": "pbts -o src/shared/signaling.d.ts src/shared/signaling.js",
    "generate-signaling": "npm run generate-signaling-js & npm run generate-signaling-ts",
    "preexample": "npm run generate-example",
    "example": "webpack --watch --config example/webpack.config.js",
    "fmt": "prettier --write \"**/*.{js,json,md,js.flow}\"",
    "flow": "flow",
    "prepublish": "npm run build:server",
    "build:server": "parcel build src/server/index.js --target=node --detailed-report --no-minify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@dlghq/dialog-api": "0.6.4",
    "@grpc/proto-loader": "0.3.0",
    "base64-js": "1.3.0",
    "body-parser": "1.18.3",
    "example": "0.0.0",
    "express": "4.16.4",
    "express-ws": "4.0.0",
    "glob": "^7.1.3",
    "grpc": "1.16.1",
    "husky": "^1.3.1",
    "kefir": "3.8.5",
    "lint-staged": "^8.1.0",
    "lodash": "4.17.11",
    "nanoevents": "^1.0.8",
    "nanoid": "2.0.0",
    "npm": "6.5.0",
    "parse-glob": "^3.0.4",
    "pino": "^5.10.4",
    "pino-pretty": "^2.5.0",
    "protocol-buffers-schema": "3.3.2",
    "run": "1.4.0",
    "rxjs": "^6.3.3",
    "ws": "6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "7.2.5",
    "@dlghq/babel-preset-dialog": "4.3.3",
    "@dlghq/eslint-config-dialog": "4.0.6",
    "babel-loader": "8.0.5",
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "0.90.0",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.9",
    "nodemon-webpack-plugin": "4.0.7",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "1.16.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0",
    "webpack-shell-plugin-next": "^0.6.4"
  }
}
