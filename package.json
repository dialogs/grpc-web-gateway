{
  "name": "@dlghq/grpc-web-gateway",
  "version": "0.0.0",
  "description": "gRPC to json gateway",
  "main": "src/index.js",
  "scripts": {
    "test": "exit 0",
    "generate-example": "pbjs -t static-module --es6 -o example/client/api.gen.js example/proto/*.proto",
    "generate-signaling-js": "pbjs -t static-module --es6 -o src/shared/signaling.js src/shared/signaling.proto",
    "generate-signaling-ts": "pbts -o src/shared/signaling.d.ts src/shared/signaling.js",
    "generate-signaling": "npm run generate-signaling-js & npm run generate-signaling-ts",
    "preexample": "npm run generate-example",
    "example": "webpack --watch --config example/webpack.config.js",
    "fmt": "prettier --write \"**/*.{js,json,md,js.flow}\"",
    "flow": "flow"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/proto-loader": "0.3.0",
    "base64-js": "1.3.0",
    "body-parser": "1.18.3",
    "example": "0.0.0",
    "express": "4.16.4",
    "express-ws": "4.0.0",
    "glob": "^7.1.3",
    "kefir": "3.8.5",
    "lodash": "4.17.11",
    "nanoevents": "^1.0.8",
    "nanoid": "2.0.0",
    "npm": "6.5.0",
    "parse-glob": "^3.0.4",
    "pino": "^5.10.4",
    "pino-pretty": "^2.5.0",
    "protocol-buffers-schema": "3.3.2",
    "run": "1.4.0",
    "rxjs": "6.3.3",
    "ws": "6.1.2"
  },
  "devDependencies": {
    "@babel/polyfill": "7.2.5",
    "@dlghq/babel-preset-dialog": "4.3.1",
    "@dlghq/eslint-config-dialog": "3.4.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.4",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "3.4.0",
    "eslint-plugin-react": "7.12.3",
    "flow-bin": "0.90.0",
    "grpc": "1.16.1",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.9",
    "nodemon-webpack-plugin": "4.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0",
    "webpack-shell-plugin-next": "^0.6.4"
  }
}
