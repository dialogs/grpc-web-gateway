{
  "name": "@dlghq/grpc-web-gateway",
  "version": "1.0.0-beta.14",
  "description": "gRPC to json gateway",
  "main": "dist/index.js",
  "scripts": {
    "test": "exit 0",
    "fmt": "prettier --write \"**/*.{js,json,md,js.flow,ts}\"",
    "flow": "flow --retries=20 --timeout=300",
    "example": "lerna exec --scope example -- npm start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npx prettier --write",
      "git add"
    ]
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "devDependencies": {
    "@dlghq/eslint-config-dialog": "4.0.10",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-react": "7.16.0",
    "flow-bin": "0.112.0",
    "html-webpack-plugin": "3.2.0",
    "lerna": "3.19.0",
    "nodemon": "2.0.0",
    "nodemon-webpack-plugin": "4.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1"
  }
}
